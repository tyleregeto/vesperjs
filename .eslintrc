{
    "rules": {
        "indent": [2, 4],
        "quotes": [2, "single"],
        "linebreak-style": [1, "unix"],
        "semi": [2, "always"],
		"no-unused-vars": [2, {"vars": "local", "args": "after-used"}],
        "comma-dangle": [1, "always-multiline"],
        "no-warning-comments": 1
    },
    "env": {
        "es6": false,
        "browser": true
    },
    "extends": "eslint:recommended",
	"globals": {}
}
